{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "classic",
		"target": "ESNext",
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"sourceMap": true,
		"strict": true,
		"alwaysStrict": true,
		"allowUnreachableCode": false,
		"strictBindCallApply": true,
		"noImplicitThis": true,
	},
	"exclude": [
		"node_modules",
		"**/node_modules/*"
	]
}